        <UseWPF>true</UseWPF>
    </PropertyGroup>

    <ItemGroup>
      <ProjectReference Include="..\GoGame.Models\GoGame.Models.csproj" />
      <ProjectReference Include="..\GoGame.Utility\GoGame.Utility.csproj" />
    </ItemGroup>

    <ItemGroup>
      <Compile Update="AssemblyInfo.cs">
