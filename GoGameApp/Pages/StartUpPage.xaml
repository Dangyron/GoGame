<Page x:Class="GoGameApp.Pages.StartUpPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:GoGameApp.Pages"
      mc:Ignorable="d"
      Title="StartUpPage" Height="450" Width="800">
    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
        <Button Visibility="Hidden" Name="Button0" Width="250" Height="0" Margin="0, 0, 0, 10" Content="Continue game" FontSize="16"/>
        <Button Name="Button1" Width="250" Height="50" Margin="0, 0, 0, 10" Content="Start game" FontSize="16"/>
        <Button Name="Button2" Width="250" Height="50" Margin="0, 0, 0, 10" Content="Settings" FontSize="16"/>
        <Button Name="Button4" Width="250" Height="50" Margin="0, 0, 0, 10" Content="Additionally" FontSize="16"/>
        <Button Name="Button3" Width="250" Height="50" Content="Exit" FontSize="16"/>
    </StackPanel>
</Page>
